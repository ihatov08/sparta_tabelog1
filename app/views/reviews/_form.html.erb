<%= form_for([@restaurant, @review]) do |f| %>
 <div class="form-group">
  <%= f.label :body, 'レビュー' %><br>
  <%= f.text_area :body, class: "form-controll" %>
 </div>
 <div class="form-group">
  <%= f.label :rate, '評価' %><br>
  <%= f.text_field :rate, class: "form-controll" %>
 </div>
 <%= f.submit %>
 <% end %>
