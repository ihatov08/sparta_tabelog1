<%= form_for([@restaurant, @review]) do |f| %>
 <div class="form-group">
  <%= f.label :body, 'レビュー' %><br>
  <%= f.text_area :body, class: "form-controll" %>
 </div>
 <div class="form-group">
  <%= f.label :rate, '評価' %><br>
  <%= f.select :rate, {"１点" => "1", "２点" => "2", "３点" => "3", "４点" => "4", "５点" => "5"}  %>


 </div>
 <%= f.submit "レビューを投稿する" %>
 <% end %>

</div>
